using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp4
{

    internal class Program
    {
        static void Main(string[] args)
        {
            int C = int.Parse(Console.ReadLine());

            double F = (double)9 / 5 * C + 32;
            Console.WriteLine(F);
        }
    }
}
