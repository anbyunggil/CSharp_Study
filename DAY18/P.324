#1.
CREATE TABLE EMP_HW(
    EMPNO NUMBER(4),
    ENAME VARCHAR(10),
    JOB VARCHAR2(9),
    MGR NUMBER(4),
    HIREDATE DATE,
    SAL NUMBER(7, 2),
    COMM NUMBER(7, 2),
    DEPTNO NUMBER(2)
);

#2.
ALTER TABLE EMP_HW
    ADD BIGO VARCHAR2(20);
SELECT * FROM EMP_HW;

#3.
ALTER TABLE EMP_HW
    MODIFY BIGO VARCHAR(30);
DESC EMP_HW;

#4.
ALTER TABLE EMP_HW
    RENAME COLUMN BIGO TO REMARK;
SELECT * FROM EMP_HW;

#5.
INSERT INTO EMP_HW
SELECT EMPNO, ENAME, JOB, MGR, HIREDATE, SAL, COMM, DEPTNO, NULL
FROM EMP;

SELECT * FROM EMP_HW;

#6.
DROP TABLE EMP_HW;
